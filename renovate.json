{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "enabled": false
    },
    {
      "matchPackagePrefixes": [
        "@daangn/webview-auth-"
      ],
      "groupName": "Webview Auth",
      "enabled": true
    },
    {
      "matchPackageNames": [
        "plantae"
      ],
      "matchPackagePrefixes": [
        "@plantae/",
        "@daangn/plantae-"
      ],
      "groupName": "Plantae",
      "enabled": true
    },
    {
      "matchPackagePrefixes": [
        "@stackflow/",
        "@daangn/stackflow-"
      ],
      "groupName": "Stackflow",
      "enabled": true
    },
    {
      "matchPackagePrefixes": [
        "@seed-design/",
        "@daangn/sprout-"
      ],
      "groupName": "Seed Design",
      "enabled": true
    },
    {
      "matchPackageNames": [
        "@daangn/karrotbridge"
      ],
      "groupName": "Karrot Bridge",
      "enabled": true
    }
  ],
  "hostRules": [
    {
      "matchHost": "https://npm.pkg.github.com/",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "npmrc": "@daangn:registry=https://npm.pkg.github.com/"
}
